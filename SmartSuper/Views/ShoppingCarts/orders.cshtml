@model IEnumerable<SmartSuper.ViewModel.PaidOrders>

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>


<table class="table">
    <tr>
        <th class="tableH">
            @Html.DisplayNameFor(model => model.customer_ID)
        </th>
        <th class="tableH">
            @Html.DisplayNameFor(model => model.Customer_Name)
        </th>
        <th class="tableH">
            @Html.DisplayNameFor(model => model.Amount_Paid)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.customer_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Customer_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount_Paid)
            </td>
        </tr>
    }

</table>
